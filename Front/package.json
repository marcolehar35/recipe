{
  "name": "react-modele",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js",
    "clean": "rm -rf dist",
    "clean:all": "rm -rf dist node_modules yarn.lock package-lock.json",
    "test": "NODE_PATH=./ mocha --require tests/.setup.js tests/**/*.test.js",
    "test:watch": "NODE_PATH=./ mocha --watch --require tests/.setup.js tests/**/*.test.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  },
  "devDependencies": {
    "@babel/core": "*",
    "@babel/plugin-proposal-class-properties": "*",
    "@babel/plugin-proposal-object-rest-spread": "*",
    "@babel/preset-env": "*",
    "@babel/preset-react": "*",
    "@babel/register": "^7.8.3",
    "babel-eslint": "*",
    "babel-loader": "^8",
    "bundle-stats": "*",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "*",
    "copy-webpack-plugin": "*",
    "cross-env": "*",
    "css-loader": "*",
    "cssnano": "*",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "*",
    "eslint-config-airbnb": "*",
    "eslint-import-resolver-alias": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "file-loader": "*",
    "html-webpack-plugin": "*",
    "husky": "^4.2.3",
    "ignore-styles": "^5.0.1",
    "mini-css-extract-plugin": "*",
    "mocha": "^7.0.1",
    "optimize-css-assets-webpack-plugin": "*",
    "postcss-loader": "*",
    "postcss-preset-env": "*",
    "resolve-url-loader": "*",
    "sass": "*",
    "sass-loader": "*",
    "style-loader": "*",
    "terser-webpack-plugin": "*",
    "webpack": "^4.41",
    "webpack-cli": "^3.3",
    "webpack-dev-server": "^3.10",
    "webpack-merge": "^4.2"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "prop-types": "*",
    "react": "*",
    "react-dom": "*",
    "react-feather": "^2.0.3",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "slugify": "^1.3.6",
    "styled-components": "*"
  }
}
